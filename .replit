modules = ["lua-5.2", "nix", "rust-nightly", "docker", "bash"]
[agent]
expertMode = true

[nix]
channel = "unstable"
packages = ["nushell", "neovim"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Nushell"

[[workflows.workflow]]
name = "Nushell"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "nu"

[workflows.workflow.metadata]
outputType = "console"
